[color]
    ui = auto
[alias]
    tohead = reset --hard HEAD
    rebc = rebase --continue
    rebs = rebase --skip
    ll = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%n' --abbrev-commit --date=relative --branches -n 10
    lln = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%n' --abbrev-commit --date=relative --branches -n
    ls = show --name-only
    fp = fetch --prune
[push]
    default = current
